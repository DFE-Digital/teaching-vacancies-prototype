
{% extends "layout.njk" %}

{# {% set serviceName = 'Manage teaching jobs' %} #}

{% set primaryNavId = 'profile' %}

{% set title = [profile.firstName, profile.lastName] | join(' ') %}

{% block beforeContent %}
  {# {{ govukBackLink({
    text: "Back to profile",
    href: "/profile"
  }) }} #}

  {{ govukBreadcrumbs({
    items: [{
      text: 'Profile',
      href: '/profile'
    }, {
      text: 'Preview'
    }]
  }) }}
{% endblock %}

{% block content %}

  {{ govukNotificationBanner({
    html: "<h2 class=\"govuk-heading-m\">"+flash+"</h2>",
    type: "success",
    icon: false
  }) if flash and flash.length }}

  <div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">

      <h1 class="govuk-heading-xl govuk-!-margin-bottom-4">
        {{title}}
      </h1>









    </div>
  </div>

{% endblock %}
