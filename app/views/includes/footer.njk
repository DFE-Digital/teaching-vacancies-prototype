{% if data.realFooter == '1' %}
  {% set items = [{
    href: "/jobs-by-type-of-school-and-role",
    text: "Jobs by type of school and role"
  },
  {
    href: "/landing",
    text: "School and teaching jobs by location"
  },
  {
    href: "/landing",
    text: "Teaching jobs by subject"
  }] %}
{% else %}
  {% set items = [{
    href: "https://govuk-prototype-kit.herokuapp.com/",
    text: "GOV.UK Prototype Kit " + releaseVersion
  },
  {
    href: '/account/sign-in-as-profile',
    text: 'Sign in as profile'
  },
  {
    href: "/prototype-admin/clear-data",
    text: "Clear data"
  },
  {
